{
  "name": "jrrdnx/craft-cloudflare-r2",
  "description": "Cloudflare R2 integration for Craft CMS",
  "type": "craft-plugin",
  "version": "0.5.0",
  "keywords": [
    "s3",
    "cms",
    "craftcms",
    "flysystem",
	"cloudflare",
    "r2",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jarrod D Nix",
      "homepage": "https://jarrodnix.dev/"
    }
  ],
  "support": {
    "issues": "https://github.com/jrrdnx/craft-cloudflare-r2/issues",
    "docs": "https://jarrodnix.dev/plugins/cloudflare-r2/v1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2.5",
    "craftcms/cms": "^3.4.0",
    "league/flysystem-aws-s3-v3": "^1.0.28"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "jrrdnx\\cloudflarer2\\": "src/"
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "extra": {
    "name": "Cloudflare R2",
    "handle": "cloudflare-r2",
    "documentationUrl": "https://jarrodnix.dev/plugins/cloudflare-r2/v1"
  },
  "config": {
    "platform": {
      "php": "7.2.5"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
